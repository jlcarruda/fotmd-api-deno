{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch via npm",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "--inspect",
        "--allow-net",
        "--allow-env",
        "--allow-write",
        "--allow-read",
        "--allow-plugin",
        "--unstable",
        "--config",
        "./tsconfig.json",
        "app.ts"
      ],
      "port": 9229
    }
  ]
}